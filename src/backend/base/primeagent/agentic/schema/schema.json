{
    "nudges": [
      {
        "id": "welcomeNudge",
        "display_text": "Welcome to Primeagent Assistant!",
        "description": "Let’s help you start building your first Primeagent app.",
        "type": "route",
        "trigger": {
          "action": "navigate",
          "payload": { "path": "/onboarding" }
        },
        "ui": {
          "icon": "sparkles",
          "variant_type": "pill",
          "variant_size": "small"
        }
      },
      {
        "id": "createFlow",
        "display_text": "Create a New Flow",
        "description": "Start a new Primeagent project to design your agent pipeline.",
        "type": "route",
        "trigger": {
          "action": "navigate",
          "payload": { "path": "/flows/new" }
        },
        "ui": {
          "icon": "plus-circle",
          "variant_type": "pill",
          "variant_size": "small"
        }
      },
      {
        "id": "openComponent",
        "display_text": "Browse Components",
        "description": "Explore and add new components to your Primeagent workspace.",
        "type": "route",
        "trigger": {
          "action": "navigate",
          "payload": { "path": "/components" }
        },
        "ui": {
          "icon": "puzzle",
          "variant_type": "pill",
          "variant_size": "small"
        }
      },
      {
        "id": "openLanguageModelComponent",
        "display_text": "Open LanguageModelComponent",
        "description": "Quickly view and configure the LanguageModelComponent in Primeagent.",
        "type": "route",
        "trigger": {
          "action": "navigate",
          "payload": { "path": "/components/LanguageModelComponent" }
        },
        "ui": {
          "icon": "brain-circuit",
          "variant_type": "pill",
          "variant_size": "small"
        }
      },
      {
        "id": "exploreTemplates",
        "display_text": "Explore Templates",
        "description": "Jumpstart your project with pre-built Primeagent templates.",
        "type": "route",
        "trigger": {
          "action": "navigate",
          "payload": { "path": "/templates" }
        },
        "ui": {
          "icon": "layout-template",
          "variant_type": "pill",
          "variant_size": "small"
        }
      },
      {
        "id": "docsNudge",
        "display_text": "Read Primeagent Docs",
        "description": "Learn about nodes, connections, and advanced flow building.",
        "type": "button_preview",
        "trigger": {
          "action": "navigate",
          "payload": { "path": "https://docs-primeagent.khulnasoft.com" }
        },
        "ui": {
          "icon": "book-open",
          "variant_type": "pill",
          "variant_size": "small"
        }
      },
      {
        "id": "feedbackPrompt",
        "display_text": "Share Feedback",
        "description": "Have ideas or issues with your Primeagent experience? Let’s improve together.",
        "type": "chat_input",
        "trigger": {
          "action": "send_message",
          "payload": {
            "text": "I’d like to share feedback about my Primeagent experience."
          }
        },
        "ui": {
          "icon": "message-square",
          "variant_type": "pill",
          "variant_size": "small"
        }
      }
    ]
  }
  